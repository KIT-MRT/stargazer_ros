<launch>

  <node pkg="stargazer_ros_tool" type="rviz_visualizer_node" name="visualizer_node" output="screen" required="true">
    <param name="stargazer_cfg_file" value="$(find stargazer_ros_tool)/res/stargazer_optimized.yaml"/>
    <param name="cereal_state_file" value="$(find stargazer_ros_tool)/res/poses_optimized.xml"/>
    <!--<param name="stargazer_cfg_file" value="/home/bandera/Desktop/res/stargazer_optimized.yaml"/>-->
    <!--<param name="cereal_state_file" value="/home/bandera/Desktop/res/poses_optimized.xml"/>-->
    <!--<param name="stargazer_cfg_file" value="$(find stargazer_ros_tool)/res/stargazer_org.yaml"/>-->
    <!--<param name="cereal_state_file" value="$(find stargazer_ros_tool)/res/poses_org.xml"/>-->
  </node>

  <!-- Start an rviz node with a custom configuration for the viewpoint, map_server, trajectory, laser scans, etc -->
  <node pkg="rviz" type="rviz" output="screen" name="rviz"/>

</launch>
